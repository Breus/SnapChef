    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.3.0",
    "globals": "^16.3.0",
    "prettier": "3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "stylelint": "^16.21.1",
