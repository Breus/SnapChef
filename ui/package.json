  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "lint:js": "eslint \"src/**/*.{js,ts,vue}\"",
    "lint:style": "stylelint \"src/**/*.{css,scss,vue}\"",
